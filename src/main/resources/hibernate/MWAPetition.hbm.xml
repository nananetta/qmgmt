<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping default-access="field">
	<class name="com.konkow.framework.domain.master.Petition" table="MWA_M_PETITION" lazy="false" mutable="false">
		<cache usage="read-only" region="com.konkow.framework.domain.master.Petition"/>
		<id name="id" type="int">
			<column name="ID" />
			<generator class="native" />
		</id>
		<version name="rowVersion" column="ROW_VERSION" />

		<many-to-one name="branch" class="com.konkow.framework.domain.master.Branch" fetch="join">
            <column name="BRANCH_ID" not-null="true" />
        </many-to-one>
		<many-to-one name="week" class="com.konkow.framework.domain.master.Week" fetch="join">
            <column name="WEEK_ID" not-null="true" />
        </many-to-one>
        
        <property name="accountNo" column="ACCOUNT_NO" />
		<property name="petitionNo" column="PETITION_NO" />
		<property name="petitionDate" column="PETITION_DATE" />

		<property name="lastUpdateDate" column="LASTUPDATE_DATE" />
		<property name="lastUpdateBy" column="LASTUPDATE_BY" />
		<property name="createDate" column="CREATE_DATE" />
		<property name="createBy" column="CREATE_BY" />
	</class>
</hibernate-mapping>
