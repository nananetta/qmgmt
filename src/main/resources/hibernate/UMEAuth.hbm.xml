<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping default-access="field">
	<class name="com.konkow.framework.domain.ume.Authorization" table="UME_M_AUTHORIZATION"  mutable="false">
		<cache usage="read-only" region="com.konkow.framework.domain.ume.Authorization"/>
		<id name="id" type="long">
			<column name="ID" />
			<generator class="native" />
		</id>
		<version name="rowVersion" column="ROW_VERSION" />

		<property name="authCode" column="AUTH_CODE" />
		<property name="authName" column="AUTH_NAME" />
		<property name="authObjectCode" column="AUTH_OBJECT_CODE" />
		
		<property name="authField1ValueFrom" column="AUTH_FIELD_1_VALUE_FROM" />
		<property name="authField1ValueTo" column="AUTH_FIELD_1_VALUE_TO" />
		
		<property name="authField2ValueFrom" column="AUTH_FIELD_2_VALUE_FROM" />
		<property name="authField2ValueTo" column="AUTH_FIELD_2_VALUE_TO" />
		
		<property name="authField3ValueFrom" column="AUTH_FIELD_3_VALUE_FROM" />
		<property name="authField3ValueTo" column="AUTH_FIELD_3_VALUE_TO" />
		
		<property name="authField4ValueFrom" column="AUTH_FIELD_4_VALUE_FROM" />
		<property name="authField4ValueTo" column="AUTH_FIELD_4_VALUE_TO" />
		
		<property name="authField5ValueFrom" column="AUTH_FIELD_5_VALUE_FROM" />
		<property name="authField5ValueTo" column="AUTH_FIELD_5_VALUE_TO" />

		<property name="lastUpdateDate" column="LASTUPDATE_DATE" />
		<property name="lastUpdateBy" column="LASTUPDATE_BY" />
		<property name="createDate" column="CREATE_DATE" />
		<property name="createBy" column="CREATE_BY" />
	</class>
</hibernate-mapping>
